datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model PlugDuck_PatCount {
  userId String @id
  pats   Int    @default(0)
}

model PlugCt_Score {
  userId String @id

  pointsThisCycle Int @default(0)
  pointsLastCycle Int @default(0)
}

model PlugPets_Pet {
  id      Int     @id @default(autoincrement())
  guildId String
  userId  String
  active  Boolean @default(true)
  url     String
}
