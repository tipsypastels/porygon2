// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Headpat {
  user_id String @id
  score   Int    @default(0)
}

model PokecomJoinDate {
  user_id   String   @id
  joined_at DateTime
}
